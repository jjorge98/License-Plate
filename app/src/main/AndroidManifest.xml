<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.licenseplate">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".HelpLicenseRequest"></activity>
        <activity android:name=".LicenseHistory" />
        <activity android:name=".UserList" />
        <activity android:name=".UserRegister" />
        <activity android:name=".forgotPassword" />
        <activity android:name=".requestListStamper" />
        <activity android:name=".requestLicense" />
        <activity android:name=".loginStamper" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>